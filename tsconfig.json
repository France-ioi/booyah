{
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ],
  "compilerOptions":{
    "alwaysStrict":true,
    "noImplicitAny": true,
    "allowSyntheticDefaultImports":true,
    "declaration":true,
    "inlineSourceMap":true,
    "lib": [
      "ESNext",
      "dom"
    ],
    "listEmittedFiles":true,
    "module": "commonjs",
    "moduleResolution":"Node",
    "outDir":"booyah",
    "pretty":true,
    "target":"ES2016"
  }
}